  "scripts": {
    "test": "cross-env NODE_ENV=API mocha -t 10000 -s 1000 --growl --colors --harmony --recursive --compilers js:babel-core/register --require babel-polyfill ./test/api/commit.test.js",
    "test:watch": "npm test -- --watch",
    "build:release": "cross-env NODE_ENV=production webpack --config ./webpack.config.production.js --progress --colors --display-error-details",
    "build:development": "cross-env NODE_ENV=development webpack --config ./webpack.config.development.js",
    "build": "npm run build:release",
    "lint": "eslint ./constants/**/*.js ./client/modules/**/*.js ./test/**/*/js --ext *.less",
    "start": "cross-env NODE_ENV=development babel-node serverSide/app 8080",
    "start:dev": "webpack-dev-server --port 8080 --hot --host 0.0.0.0 --config webpack.config.development.js"
